    <file href="application/javascript/jquery.ui.datepicker.min.js" type="text/javascript" />
    <file href="custom/extensions/Veranstaltungen/js/datepicker-de.js" type="text/javascript" />
    <field name="Detail-Seite" key="detail" type="page_id" length="int" width="300" height="1">
      <wrap><![CDATA[{page_link[|]}]]></wrap>
    </field>
    <field name="Kalender-Seite" key="calendar" type="page_id" length="int" width="300" height="1">
      <wrap><![CDATA[{page_link[|]}]]></wrap>
    </field>
  </object>

</objects>